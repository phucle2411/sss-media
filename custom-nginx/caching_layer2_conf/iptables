iptables -I INPUT -s 103.60.16.121 -p tcp --match multiport --dports 80,9100 -j ACCEPT
iptables -I INPUT -s 103.60.16.19 -p tcp --match multiport --dports 80,9100 -j ACCEPT 
iptables -I INPUT -s 192.168.0.0/16 -p tcp --match multiport --dports 80,9100 -j ACCEPT
iptables -I INPUT -s 10.0.0.0/8 -p tcp --match multiport --dports 80,9100  -j ACCEPT
iptables -I INPUT -s 172.16.0.0/12 -p tcp --match multiport --dports 80,9100  -j ACCEPT
iptables -I INPUT -s 127.0.0.1 -p tcp --match multiport --dports 80,9100  -j ACCEPT
iptables -A INPUT -s 0.0.0.0/0 -p tcp --match multiport --dports 80,9100  -j DROP 